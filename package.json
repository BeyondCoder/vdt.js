{
  "name": "vdt",
  "version": "0.3.12",
  "description": "A template engine based on virtual-dom",
  "main": "src/index",
  "scripts": {
    "build": "browserify src/browserify.js -o dist/vdt.js -s Vdt && uglifyjs dist/vdt.js -o dist/vdt.min.js -m",
    "test": "node_modules/.bin/mocha --compilers coffee:coffee-script/register test/*.coffee; node_modules/.bin/mocha-phantomjs test/browser/index.html",
    "publish": "node publish.js"
  },
  "dependencies": {
    "dom-delegator": "^13.1.0",
    "virtual-dom": "git://github.com/Javey/virtual-dom.git#master"
  },
  "devDependencies": {
    "benchmark": "^1.0.0",
    "coffee-script": "^1.9.2",
    "handlebars": "^3.0.2",
    "jquery": "^2.1.3",
    "mocha": "^2.2.4",
    "mocha-phantomjs": "^4.0.1",
    "mustache": "^2.0.0",
    "platform": "^1.3.0",
    "should": "^6.0.1",
    "todomvc-app-css": "^2.0.1",
    "todomvc-common": "^1.0.1",
    "underscore": "^1.8.3"
  }
}
